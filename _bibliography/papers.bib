@article{Cunha2016,
abstract = {Channels composed by Einstein-Podolsky-Rosen (EPR) pairs are capable of teleporting arbitrary multipartite states. The question arises whether EPR channels are also optimal against imperfections. In particular, the teleportation of Greenberger-Horne-Zeilinger states (GHZ) requires three EPR states as the channel and full measurements in the Bell basis. We show that, by using two GHZ states as the channel, it is possible to transport any unknown three-qubit state of the form $c_0|000\rangle+c_1|111\rangle$. The teleportation is made through measurements in the GHZ basis, and, to obtain deterministic results, in most of the investigated scenarios, four out of the eight elements of the basis need to be unambiguously distinguished. Most importantly, we show that when both, systematic errors and noise are considered, the fidelity of the teleportation protocol is higher when a GHZ channel is used in comparison to that of a channel composed by EPR pairs.},
archivePrefix = {arXiv},
arxivId = {1611.01167},
author = {Cunha, M{\'{a}}rcio M. and Fonseca, E. A. and Parisio, Fernando},
doi = {10.1007/s11128-017-1705-9},
eprint = {1611.01167},
file = {:home/alejo/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Cunha, Fonseca, Parisio - 2016 - Non-ideal teleportation of tripartite entanglement Einstein-Podolsky-Rosen versus Greenberger-Horne-Zei.pdf:pdf},
issn = {1570-0755},
journal = {Quantum Information Processing},
keywords = {Entanglement,Noise,Teleportation},
month = {nov},
number = {10},
pages = {254},
title = {{Non-ideal teleportation of tripartite entanglement: Einstein-Podolsky-Rosen versus Greenberger-Horne-Zeilinger schemes}},
url = {http://link.springer.com/10.1007/s11128-017-1705-9 http://arxiv.org/abs/1611.01167 http://dx.doi.org/10.1007/s11128-017-1705-9},
volume = {16},
year = {2016}
}

@article{Fonseca2018,
abstract = {The question of how Bell nonlocality behaves in bipartite systems of higher dimensions is ad-dressed. By employing the probability of violation of local realism under random measurements as the figure of merit, we investigate the nonlocality of entangled qudits with dimensions ranging from d = 2 to d = 7. We proceed in two complementary directions. First, we study the specific Bell scenario defined by the Collins-Gisin-Linden-Massar-Popescu (CGLMP) inequality. Second, we consider the nonlocality of the same states under a more general perspective, by directly addressing the space of joint probabilities (computing the frequencies of behaviours outside the local polytope). In both approaches we find that the nonlocality decreases as the dimension d grows, but in quite distinct ways. While the drop in the probability of violation is exponential in the CGLMP scenario, it presents, at most, a linear decay in the space of behaviours. Furthermore, in both cases the states that produce maximal numeric violations in the CGLMP inequality present low probabilities of violation in comparison with maximally entangled states, so, no anomaly is observed. Finally, the nonlocality of states with non-maximal Schmidt rank is investigated.},
archivePrefix = {arXiv},
arxivId = {1805.09451},
author = {Fonseca, Alejandro and de Rosier, Anna and V{\'{e}}rtesi, Tam{\'{a}}s and Laskowski, Wies{\l}aw and Parisio, Fernando},
doi = {10.1103/PhysRevA.98.042105},
eprint = {1805.09451},
file = {:home/alejo/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Fonseca et al. - 2018 - Survey on the Bell nonlocality of a pair of entangled qudits.pdf:pdf},
issn = {2469-9926},
journal = {Physical Review A},
mendeley-groups = {Qudits},
month = {oct},
number = {4},
pages = {042105},
publisher = {American Physical Society},
title = {{Survey on the Bell nonlocality of a pair of entangled qudits}},
url = {https://link.aps.org/doi/10.1103/PhysRevA.98.042105},
volume = {98},
year = {2018}
}

@article{Moreno2018,
abstract = {The problem of noise incidence on qubits taking part of bipartite entanglement-based protocols is addressed. It is shown that the use of a three-partite GHZ state and measurements instead of their EPR counterparts allows the experimenter to detect 2/3 of the times whenever one of the qubits involved in the measurement is affected by bit-flip noise through the mere observation of unexpected outcomes in the teleportation and superdense coding protocols when compared to the ideal case. It is shown that the use of post-selection after the detection of noise leads to an enhancement in the efficiency of the protocols. The idea is extended to any protocol using entangled states and measurements. Furthermore, a generalization is provided in which GHZ states and measurements with an arbitrary amount of qubits are used instead of EPR pairs, and remarkably, it is concluded that the optimal number of qubits is only three.},
archivePrefix = {arXiv},
arxivId = {1805.06517},
author = {Moreno, M. G.M. and Fonseca, Alejandro and Cunha, M{\'{a}}rcio M.},
doi = {10.1007/s11128-018-1960-4},
eprint = {1805.06517},
file = {:home/alejo/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Moreno, Fonseca, Cunha - 2018 - Using three-partite GHZ states for partial quantum error detection in entanglement-based protocols.pdf:pdf},
issn = {15700755},
journal = {Quantum Information Processing},
keywords = {Entanglement,Error detection,GHZ states},
mendeley-groups = {Teleportation},
month = {aug},
number = {8},
pages = {191},
publisher = {Springer US},
title = {{Using three-partite GHZ states for partial quantum error detection in entanglement-based protocols}},
url = {http://link.springer.com/10.1007/s11128-018-1960-4},
volume = {17},
year = {2018}
}

@article{Cunha2019,
abstract = {We review some current ideas of tripartite entanglement. In particular, we consider the case representing the next level of complexity beyond the simplest (though far from trivial) one, namely the bipartite case. This kind of entanglement plays an essential role in understanding the foundations of quantum mechanics. It also allows for implementing several applications in the fields of quantum information processing and quantum computing. In this paper, we review the fundamental aspects of tripartite entanglement focusing on Greenberger–Horne–Zeilinger and W states for discrete variables. We discuss the possibility of using it as a resource to execute quantum protocols and present some examples in detail.},
author = {Cunha, M{\'{a}}rcio M. and Fonseca, Alejandro and Silva, Edilberto O.},
doi = {10.3390/universe5100209},
file = {:home/alejo/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Cunha, Fonseca, Silva - 2019 - Tripartite Entanglement Foundations and Applications.pdf:pdf},
issn = {2218-1997},
journal = {Universe},
keywords = {GHZ states,W states,quantum entanglement,quantum mechanics,tripartite entanglement},
mendeley-groups = {Multipartite Entanglement},
month = {oct},
number = {10},
pages = {209},
publisher = {Multidisciplinary Digital Publishing Institute},
title = {{Tripartite Entanglement: Foundations and Applications}},
url = {https://www.mdpi.com/2218-1997/5/10/209},
volume = {5},
year = {2019}
}

@article{Fonseca2019,
abstract = {We study the protocol of qudit teleportation using quantum systems subjected to several kinds of noise for arbitrary dimensionality d. We consider four classes of noise: Dit-flip, d-phase-flip, dit-phase-flip, and depolarizing noise, each of them corresponding to a family of Weyl operators, introduced via Kraus formalism. We derive a general expression for the average fidelity of teleportation in arbitrary dimension d for any combination of noise on the involved qudits. Under a different approach we derive the average fidelity of teleportation for a more general scenario involving the d-dimensional generalization of amplitude damping noise as well. We show that all possible scenarios may be classified in four different behaviors and discuss the cases in which it is possible to improve the fidelity by increasing the associated noise fractions. All our results are in agreement with previous analysis for the case of qubits [R. Fortes and G. Rigolin, Phys. Rev. A 92, 012338 (2015)PLRAAN1050-294710.1103/PhysRevA.92.012338].},
archivePrefix = {arXiv},
arxivId = {1908.01097},
author = {Fonseca, Alejandro},
doi = {10.1103/PhysRevA.100.062311},
eprint = {1908.01097},
file = {:home/alejo/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Fonseca - 2019 - High-dimensional quantum teleportation under noisy environments.pdf:pdf},
issn = {2469-9926},
journal = {Physical Review A},
mendeley-groups = {Qudits,Teleportation},
month = {dec},
number = {6},
pages = {062311},
publisher = {American Physical Society},
title = {{High-dimensional quantum teleportation under noisy environments}},
url = {https://link.aps.org/doi/10.1103/PhysRevA.100.062311 http://arxiv.org/abs/1908.01097},
volume = {100},
year = {2019}
}

@article{DeLeon2022,
abstract = {Decoherence of quantum systems is described by quantum channels. However, a complete understanding of such channels, especially in the multiparticle setting, is still an ongoing difficult task. We propose the family of quantum maps that preserve or completely erase the components of a multiqubit system in the basis of Pauli strings, which we call Pauli component erasing maps. For the corresponding channels, it is shown that the preserved components can be interpreted as a finite vector subspace, from which we derive several properties and complete the characterization. Moreover, we show that the obtained family of channels forms a semigroup and derive its generators. We use this simple structure to determine physical implementations and connect the obtained family of channels with Markovian processes.},
archivePrefix = {arXiv},
arxivId = {2205.05808},
author = {{De Leon}, Jose Alfredo and Fonseca, Alejandro and Leyvraz, Fran{\c{c}}ois and Davalos, David and Pineda, Carlos},
