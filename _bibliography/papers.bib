file = {:home/alejo/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Basile et al. - 2023 - Weyl channels for multipartite systems(2).pdf:pdf},
issn = {2469-9926},
journal = {Physical Review A},
month = {mar},
number = {3},
pages = {032607},
title = {{Weyl channels for multipartite systems}},
url = {http://arxiv.org/abs/2310.10947 https://link.aps.org/doi/10.1103/PhysRevA.109.032607},
volume = {109},
year = {2024}
}
